<% end -%>
<% if @service_notification_commands != false -%>
  service_notification_commands     <%= @service_notification_commands %>
<% end -%>
<% if @email != false -%>
  email                             <%= @email %>
<% end -%>
<% if @pager  != false -%>
  pager                             <%= @pager %>
<% end -%>
<% if @register != false -%>
  register                          <%= @register %>
<% end -%>
}
